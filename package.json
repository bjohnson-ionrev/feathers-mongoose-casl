  "homepage": "https://github.com/doronnahum/feathers-mongoose-casl.git",
  "main": "lib/",
  "keywords": [
    "feathers",
    "casl",
    "mongoosejs"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doronnahum/feathers-mongoose-casl.git"
  },
  "author": {
    "name": "Doron nahum",
    "email": "doron.nahum@gmail.com",
